<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}

    <link href=" {{ asset('assets/css/bootstrap.min.css') }}" rel="stylesheet">
    <link href=" {{ asset('assets/css/base.css') }}" rel="stylesheet">
    </head>
    <body>
        <nav class="navbar navbar-expand-lg bg-dark">
            <div class="container-fluid">
                <a class="navbar-brand" href="{{ path('app_pokemon_gen', { 'gen' : 1 }) }}">Voir les pokemon</a>
                <a class="navbar-brand" href="{{ path('app_pokemon_show', { 'id' : 156 }) }}">Staross</a>
                <a class="navbar-brand" href="{{ path('app_guess_type_game_init') }}">Game</a>
                <a class="navbar-brand" href="{{ path('app_team') }}">Teams</a>
                <a class="navbar-brand" href="{{ path('app_register') }}">Register</a>
                <a class="navbar-brand" href="{{ path('app_login') }}">Login</a>
                <a class="navbar-brand" href="{{ path('app_logout') }}">Logout</a>
            </div>
        </nav>
        {% block body %}{% endblock %}
        <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
        <script src="{{ asset('assets/js/bootstrap.bundle.js') }}"></script>
    </body>
</html>